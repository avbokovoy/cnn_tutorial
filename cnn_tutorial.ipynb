   "source": [
    "# Define class name vector\n",
    "classes   = ( 'airplane', 'automobile', 'bird', 'cat', 'deer', 'dog', 'frog', 'horse', 'ship', 'truck' )\n",
    "# Define dataset iterator\n",
    "data_iter = iter( test_loader )\n",
    "# Get the images and labels from dataset.\n",
